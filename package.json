{
  "name": "n8n-nodes-abacus-ai",
  "version": "1.0.0",
  "description": "n8n custom node for Abacus.ai LLM prompt writing and AI operations",
  "keywords": [
    "n8n-community-node-package",
    "abacus",
    "ai",
    "llm",
    "prompt",
    "machine learning"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mikewisellc/n8n-nodes-abacus-ai",
  "author": {
    "name": "Michael Murray",
    "email": "mikewisellc@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikewisellc/n8n-nodes-abacus-ai.git"
  },
  "engines": {
    "node": ">=18.19.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AbacusAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AbacusAi/AbacusAi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  }
} 